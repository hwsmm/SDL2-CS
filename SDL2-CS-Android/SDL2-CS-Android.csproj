<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0-android</TargetFramework>
		<SupportedOSPlatformVersion>21.0</SupportedOSPlatformVersion>
		<RootNamespace>SDL2_CS_Android</RootNamespace>
		<OutputType>Library</OutputType>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<CheckEolWorkloads>false</CheckEolWorkloads>
	</PropertyGroup>
	<ItemGroup>
		<EmbeddedJar Include="Jars\SDL2AndroidBridge.jar" />
	</ItemGroup>
	<ItemGroup>
		<EmbeddedNativeLibrary Include="native\armeabi-v7a\libSDL2.so" />
		<EmbeddedNativeLibrary Include="native\armeabi-v7a\libSDL2AndroidMainSetter.so" />
		<EmbeddedNativeLibrary Include="native\arm64-v8a\libSDL2.so" />
		<EmbeddedNativeLibrary Include="native\arm64-v8a\libSDL2AndroidMainSetter.so" />
		<EmbeddedNativeLibrary Include="native\x86\libSDL2.so" />
		<EmbeddedNativeLibrary Include="native\x86\libSDL2AndroidMainSetter.so" />
	</ItemGroup>
</Project>